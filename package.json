{
  "name": "bakewise",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "format": "nx format:write",
    "format:check": "nx format:check",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "build": "nx run-many -t build",
    "serve:api": "nx serve api-gateway",
    "serve:frontend": "nx serve frontend",
    "graph": "nx graph",
    "affected": "nx affected -t lint,test,build",
    "prisma:generate": "pnpm prisma:generate:auth && pnpm prisma:generate:inventory",
    "prisma:generate:auth": "prisma generate --schema prisma/auth/schema.prisma",
    "prisma:generate:inventory": "prisma generate --schema prisma/inventory/schema.prisma",
    "prisma:db:push": "prisma db push --schema prisma/auth/schema.prisma && prisma db push --schema prisma/inventory/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
